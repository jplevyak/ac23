mm = list(map(lambda x: x.strip(), open("11", "r").readlines()))
xx = [True] * len(mm[0])
yy = [True] * len(mm)

gg = []
for y, m in enumerate(mm):
    for x, p in enumerate(m):
        if p == '#':
            gg.append((y, x))

for y, m in enumerate(mm):
    yy[y] = m.find('#') == -1
    for i, p in enumerate(m):
        xx[i] = xx[i] and p == '.'

def between(a, b, v):
    n = 0
    for i in range(min(a, b), max(a, b)):
        if v[i]:
            n += 999999
    return n

t = 0
for y, g in enumerate(gg):
    for x, h in enumerate(gg[y+1:]):
        t += abs(g[0] - h[0]) + abs(g[1] - h[1]) + between(g[1], h[1], xx) + between(g[0], h[0], yy)
print('11b', t)

from functools import cache
mm = list(map(lambda x: x.strip(), open("12", "r").readlines()))

@cache
def f(m, i, l):
    while i < len(m) and m[i] == '.':
        i += 1
    if i >= len(m):
        return 1 if len(l) == 0 else 0
    if len(l) == 0:
        while i < len(m) and m[i] != '#':
            i += 1
        return 1 if i >= len(m) else 0
    v = 0
    if m[i] == '?':
        v += f('.' + m[i+1:], 0, l) 
    p = i
    while i < len(m) and m[i] != '.' and i - p != l[0]:
        i += 1
    if i - p == l[0] and (i >= len(m) or m[i] != '#'):
        v += f('.' + m[i+1:], 0, l[1:])
    return v

t = 0
for m in mm:
    x, y = m.strip().split()
    l = list(map(int, y.split(',')))
    t += f(x, 0, tuple(l))
print('12a', t)

t = 0
for m in mm:
    x, y = m.strip().split()
    l = list(map(int, y.split(',')))
    ll = [x for _ in range(5) for x in l]
    xx = x + '?' + x + '?' + x + '?' + x + '?' + x
    t += f(xx, 0, tuple(ll))
print('12b', t)

mm = list(map(lambda x: x.strip(), open("0", "r").readlines()))
f = []
mm.append('')
t = 0
for m in mm:
    if len(m) != 0:
        f.append(m)
        continue
    c = [1] * (len(f[0]) - 1)
    c2 = [0] * (len(f[0]) - 1)
    #c2 = [(i + 1) * len(f) if i < (len(f[0]) // 2) else (len(f[0]) - i - 1) * len(f) for i in range(len(f[0]) - 1)]
    for y in range(len(f)):
        for a in range(len(f[0])):
            for b in range(a + 1, len(f[0]), 2):
                if f[y][a] != f[y][b] and c[a + ((b - a) // 2)] == 1:
                    c[a + (b - a) // 2] = 0
                if f[y][a] != f[y][b]:
                    c2[a + ((b - a) // 2)] += 1
    r = [1] * (len(f) - 1)
    r2 = [0] * (len(f) - 1)
    #r2 = [(i + 1) * len(f[0]) if i < (len(f) // 2) else (len(f) - i - 1) * len(f[0]) for i in range(len(f) - 1)]
    for x in range(len(f[0])):
        for a in range(len(f)):
            for b in range(a + 1, len(f), 2):
                if f[a][x] != f[b][x] and r[a + ((b - a) // 2)] == 1:
                    r[a + (b - a) // 2] = 0
                if f[a][x] != f[b][x]:
                    r2[a + (b - a) // 2] += 1
    print('c2', c2)
    print('r2', r2)
    if sum(c) == 1:
        t += c.index(1) + 1
    if sum(r) == 1:
        t += 100 * (r.index(1) + 1)
    f = []
print('13a', t)
